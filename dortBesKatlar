
import java.util.Scanner;
public class dörtBesKatlar {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int i,j, n;
        System.out.println("Lütfen Bir Sayı Giriniz");
        n = scan.nextInt();
        for (i = 1; i <= n; i *= 4) {
            System.out.print(i + ", ");
        }
        System.out.println("");
        for (j = 1; j <= n; j *= 5) {
                System.out.print(j + ", ");
            }
    }
}
